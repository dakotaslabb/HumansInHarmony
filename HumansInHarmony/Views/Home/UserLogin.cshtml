@model IEnumerable<HumansInHarmony.Models.User>

@{
    ViewData["Title"] = "Login";
}

<h1>Login</h1>

@ViewBag.Error

<form action="UserLogin" method="post">
    <table>
        <tr>
            <th>
                Email:<br />
            <th>
                <input type="text" name="Email" />
            </th>
        </tr>
        <tr>
            <th>
                Password:<br />
            </th>
            <th>
                <input type="password" name="Password" />
            </th>
        </tr>
        <tr>
            <th>
                <input type="submit" value="Login" />
            </th>
        </tr>
    </table>
</form>
